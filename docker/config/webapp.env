# Webapp configuration file covering behavior configuration for the webapp

# NOTE: This file shouldn't contain any environment-specific or secret
# configuration.

# Marker to denote this file's contents are in the environment.
WEBAPP_BEHAVIOR=1

# ----------------------------------------------------------------------------------
# from socorro/webapp-django
# ----------------------------------------------------------------------------------

ANALYZE_MODEL_FETCHES=True
CACHE_BACKEND=django.core.cache.backends.memcached.MemcachedCache
CACHE_KEY_PREFIX=socorro
COMPRESS_OFFLINE=True
DATASERVICE_DATABASE_NAME=breakpad
DEFAULT_PRODUCT=Firefox
NEWRELIC_PYTHON_INI_FILE=/etc/newrelic/newrelic.ini
